# .env sample for YoutubeLive DiscordBot
# Set the config.json path (absolute or relative)
CONFIG_PATH=./config/config.json

# Set the directory that contains config.json (used if CONFIG_PATH is not set)
CONFIG_DIR=./config

# Set the directory for latest.log
LOG_DIR=./logs

# Set the directory for live.json (relative to CWD if not absolute)
LIVE_JSON_DIR=./live

# YouTube API
YOUTUBE_API_KEY=your_youtube_api_key

# Discord
DISCORD_BOT_TOKEN=your_discord_bot_token
DISCORD_WEBHOOK_URL=your_discord_webhook_url
