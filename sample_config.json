      "scheduled_created",
      "scheduled_updated",
      "live_started",
      "live_ended"
    ],
    "embed": {
      "colors": {
        "scheduled_created": "yellowgreen",
        "scheduled_updated": "orange",
        "live_started": "lightskyblue",
        "live_ended": "red"
